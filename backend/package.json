{
  "name": "backend",
  "version": "0.0.1",
  "description": "A starter for Medusa projects.",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "migrate": "dotenv -e .env -- medusa db:migrate",
    "migrate:custom": "dotenv -e .env -- yarn typeorm migration:run -d ./src/datasource/data-source.ts",
    "seed:roles": "dotenv -e .env -- ts-node ./src/scripts/seed-roles.ts",
    "create:admin": "dotenv -e .env -- ts-node ./src/scripts/create-admin.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "build": "medusa build",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "start": "medusa telemetry --disable && medusa start",
    "dev": "medusa develop",
    "test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit"
  },
  "dependencies": {
    "@medusajs/event-bus-redis": "2.12.3",
    "@medusajs/admin-sdk": "2.12.3",
    "@medusajs/cli": "2.12.3",
    "@medusajs/framework": "2.12.3",
    "@medusajs/medusa": "2.12.3",
    "@stripe/react-stripe-js": "^5.4.1",
    "@types/formidable": "^3.4.6",
    "bcrypt": "^5.1.1",
    "formidable": "^3.5.4",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^7.0.12",
    "otplib": "^12.0.1",
    "pdfkit": "^0.15.0",
    "qrcode": "^1.5.4"
  },
  "devDependencies": {
    "@medusajs/test-utils": "2.12.3",
    "@swc/core": "^1.15.8",
    "@swc/jest": "^0.2.36",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.13",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^20.12.11",
    "@types/nodemailer": "^7.0.4",
    "@types/pdfkit": "^0.13.5",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "dotenv-cli": "^11.0.0",
    "jest": "^29.7.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ts-node": "^10.9.2",
    "typeorm": "^0.3.28",
    "typescript": "^5.6.2",
    "vite": "^5.4.14",
    "yalc": "^1.0.0-pre.53"
  },
  "engines": {
    "node": ">=20"
  }
}
